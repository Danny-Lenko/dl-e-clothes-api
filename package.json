{
  "name": "dl-e-clothes-api",
  "version": "1.0.0",
  "description": "A simple serverless GraphQL API built with TypeScript and deployed on Netlify.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danny-Lenko/dl-e-clothes-api.git"
  },
  "keywords": [
    "typescript",
    "graphql",
    "api",
    "netlify"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "1.12.2",
    "@graphql-codegen/typescript-resolvers": "1.12.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "apollo-server": "^2.10.1",
    "apollo-server-lambda": "^2.10.1",
    "firebase-admin": "^12.2.0"
  }
